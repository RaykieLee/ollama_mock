# OpenRouter API 配置
OPENROUTER_API_KEY=your_api_key_here

# 服务器配置
HOST=0.0.0.0
PORT=11434
LOG_LEVEL=INFO
WORKERS=4

# 数据库配置
DB_FILE=db.json
DB_BACKUP_ENABLED=true
DB_BACKUP_INTERVAL=3600

# 日志配置
LOG_FILE=logs/api.log
LOG_MAX_SIZE=10485760
LOG_BACKUP_COUNT=5
LOG_FORMAT="%(asctime)s - %(name)s - %(levelname)s - %(message)s"

# 开发配置
DEBUG=false
RELOAD=true